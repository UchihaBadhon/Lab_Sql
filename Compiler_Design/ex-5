#include<stdio.h>

//Write a C Program that will tokenize a string (Without using library function).
int main()
{
    char ch[50], temp[50];
    gets(ch);
    int cnt = 0,i = 0, j = 0;
    while( ch[i] != '\0' )
    {
        if( ch[i] != ' ' ){
            printf("%c", ch[i]);
        }
        else{
            printf("\n");
        }
        i++;
    }

}
